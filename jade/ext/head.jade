//output: 
// Meta Tags
meta(charset="utf-8")
meta(http-equiv="X-UA-Compatible" content="IE=edge")
meta(name="viewport" content="width=device-width, initial-scale=1")
meta(name="theme-color" content="#{pageColor}")

title #{title}

// Favicon URL
link(rel="icon" type="image/png" href="#{favicon}")

// jQuery
script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.0.0/jquery.min.js")
script(src="https://cdn.rawgit.com/jquery/jquery-color/master/jquery.color.js" charset="utf-8")

// Tiny Color
script(src="https://cdn.rawgit.com/bgrins/TinyColor/master/tinycolor.js" charset="utf-8")

// ClipboardJS
script(src="https://cdn.rawgit.com/zenorocha/clipboard.js/v1.5.10/dist/clipboard.min.js")

// Bootstrap Javascript
script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js")

// Underscore JS / Multiple math functions
script(src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js" charset="utf-8")

// Detect JS  
script(src="https://cdnjs.cloudflare.com/ajax/libs/Detect.js/2.2.2/detect.min.js" charset="utf-8")

// Personal JS 
script(src="js/#{pageName}.js")

// Bootstrap CSS
link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css")

// Font Awsome
link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css")

// Chili CSS
link(rel="stylesheet" href="https://rawgit.com/Rawnly/Chili.css/master/dist/chili.css")

// Personal Stylesheet
link(rel="stylesheet" href="css/#{pageName}.css")
